{
  "name": "latente",
  "version": "1.0.0",
  "description": "Sistema de co-presencias latentes (anti-engagement)",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:postgres": "npm run test:postgres:up && cross-env NODE_ENV=test ENABLE_POSTGRES_TESTS=true DATABASE_URL_TEST=postgresql://latentum_app:latentum_test_pass@127.0.0.1:55432/latentum_test jest",
    "test:postgres:up": "docker compose -f docker-compose.test.yml up -d --wait",
    "test:postgres:down": "docker compose -f docker-compose.test.yml down -v",
    "runner:demo": "ts-node scripts/runners/demoHappyPath.ts",
    "runner:sad": "ts-node scripts/runners/demoSadPaths.ts",
    "docs:api": "ts-node scripts/generate/generateApiServicios.ts",
    "cli": "ts-node scripts/cli/index.ts",
    "demo:happy": "ts-node -e \"(async () => { const { createAdapter } = require('./scripts/cli/adapter'); const { runHappyPath } = require('./scripts/cli/flows/happyPath'); const now = new Date('2026-01-10T10:00:00.000Z'); const adapter = createAdapter(); const res = await runHappyPath(adapter, now); console.log(JSON.stringify(res, null, 2)); })().catch(e=>{console.error(e);process.exit(1);});\""
  },
  "keywords": [
    "co-presence",
    "anti-engagement",
    "privacy"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^10.0.0",
    "cross-env": "^10.1.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/rate-limit": "^10.3.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "fluent-json-schema": "^6.0.0",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "uuid": "^13.0.0"
  }
}
